
<!DOCTYPE html>
<html>
  <head>
    <title>Stylists</title>
    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css'>
  </head>

  <h1>Clients</h1>
  <table class="table table-hover table-danger">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Stylist</th>

    </tr>
  </thead>
  <tbody>

  @foreach(var clients in Model)
  {
<tr>
      <td>@clients.GetName()  | <a href="/clients/@clients.GetId()/edit">Edit</a>| <a href="/clients/@clients.GetId()/delete">Delete</a></td>




</tr>
  }

  </tbody>
</table>



<br><br>
<a href="/clients/add" class="btn btn-success">Add Clients</a><br>
</html>
<a href='/stylists/add'>Add a stylist</a><br>
<a href='/clients/add'>Add a client</a><br>
<a href="/stylists">View All Stylist</a>
